<h1>
    Ironhack Cinema
</h1>
<h3>Click on the movie to check the showtimes!</h3>

<div id="container">
    {{#each movies}}
    <div class="movie">
        <img src="{{this.img}}" alt="test">
        <h3>{{this.title}}</h3>
        <a href="/movie-info/{{this._id}}">See more</a>
    </div>
    {{/each}}
</div>